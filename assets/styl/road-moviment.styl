@keyframes blue-car {
	from {
		left:-90px;
	}
	to { 
		left:3000px;
	}
}
@keyframes green-van {
	from {
		right:-90px;
	}
	to { 
		right:3000px;
	}
}
@keyframes brown-van {
	from {
		right:-600px;
	}
	to { 
		right:3000px;
	}
}
.blue-car
	top: -25px
	animation: blue-car 15s linear infinite

.green-van
	top: -10px
	animation: green-van 20s linear infinite

.brown-van
	top: -10px
	animation: brown-van 20s linear infinite